{
    "equation": "q1/(4*pi*epsilon*r**2)",
    "num_samples": 100,
    "models": [
        {
            "type": "linear",
            "model_path": "clean",
            "load": false,
            "training_type": "clean",
            "fit_args": {
                "epochs": 500,
                "early_stopping": 10
            },
            "noisy_input_feats": [
                0,
                1,
                2,
                3
